{
  "name": "kwalito-browser-extension",
  "version": "0.0.1",
  "description": "The Kwalito browser extension to filter food products on online grocery stores",
  "scripts": {
    "dev": "node scripts/dev",
    "build": "node scripts/build",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint": "eslint app chrome test scripts webpack/*.js",
    "test-e2e": "cross-env NODE_ENV=test mocha -r ./test/setup-app test/e2e",
    "test": "cross-env NODE_ENV=test mocha -r ./test/setup-app test/app",
    "test-coverage": "cross-env nyc --all npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kwalitoapp/kwalito-browser-extension"
  },
  "keywords": [
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "author": "Kwalito",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-postcss": "^0.3.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^2.33.1",
    "cross-env": "^5.1.0",
    "crx": "^3.2.1",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "enzyme": "^2.9.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "jsdom": "^11.3.0",
    "minimist": "^1.2.0",
    "nyc": "^11.3.0",
    "postcss": "^6.0.13",
    "postcss-cssnext": "^3.0.2",
    "postcss-custom-properties": "^6.2.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.8",
    "pug-cli": "^1.0.0-alpha6",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "remote-redux-devtools": "^0.5.12",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "^3.6.0",
    "shelljs": "^0.7.8",
    "style-loader": "^0.18.2",
    "webpack": "^3.8.1",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-httpolyglot-server": "^0.2.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy",
      "transform-runtime"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.4",
    "mocha": "^3.5.3",
    "normalize.css": "^7.0.0",
    "pouchdb": "^6.3.4",
    "pouchdb-authentication": "^0.5.5",
    "pouchdb-quick-search": "^1.3.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dock": "^0.2.4",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sinon": "^3.3.0"
  }
}
