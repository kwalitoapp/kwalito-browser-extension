{
  "name": "kwalito-browser-extension",
  "version": "0.0.1",
  "description": "The Kwalito browser extension to filter food products on online grocery stores",
  "scripts": {
    "dev": "node scripts/dev",
    "build": "node scripts/build",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint": "eslint app chrome test scripts webpack/*.js",
    "test-e2e": "cross-env NODE_ENV=test mocha -r ./test/setup-app test/e2e",
    "test": "cross-env NODE_ENV=test ava ./test/setup-app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kwalitoapp/kwalito-browser-extension"
  },
  "keywords": [
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "author": "Kwalito",
  "license": "GPL-3.0",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-postcss": "^0.3.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chromedriver": "^2.32.3",
    "cross-env": "^5.0.5",
    "crx": "^3.2.1",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "enzyme": "^2.9.1",
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "jsdom": "^11.2.0",
    "minimist": "^1.2.0",
    "postcss": "^6.0.11",
    "postcss-cssnext": "^3.0.2",
    "postcss-custom-properties": "^6.1.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.6",
    "pug-cli": "^1.0.0-alpha6",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "^3.5.0",
    "shelljs": "^0.7.8",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-httpolyglot-server": "^0.2.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "verbose": true,
    "files": [
      "test/**/*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy",
      "transform-runtime"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dock": "^0.2.4",
    "react-icons": "^2.2.5",
    "react-redux": "^5.0.6",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-router-redux-dom-link": "^0.2.0",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
